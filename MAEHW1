%%Question 9.12%%
%Part A%
clc;
clear all;
close all;
%Function G1(s)=(1)/(s+1)
%Numerator
num=[1];
%Denominator
den=[1 1];
%Transfer Function
G= tf(num, den)
%Plot
bode ( G ),grid 

%Part B%
clc;
clear all;
close all;
%Function G2(s)=(s-1)/(1)
%Numerator
num= [1 (-1)];
%Denominator
den= [1];
%Transform Function
G=tf(num, den)
%Plot
bode(G), grid 

%Part C%
clc;
clear all;
close all;
%Function G3(s)=(s-1)/(s+1)=G1(s)G2(s)
